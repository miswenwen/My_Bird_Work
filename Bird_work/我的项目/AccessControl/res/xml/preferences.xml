		android:title="@string/select_applicationgs" 
		android:key="select_protect_applications"
		android:summary="@string/select_summary"/>	
	<Preference 
		android:title="@string/password_protect" 
		android:key="password_protect_applicationgs"
		android:summary="@string/password_protect_summary"/>		
