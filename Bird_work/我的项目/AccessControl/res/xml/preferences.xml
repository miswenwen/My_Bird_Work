<PreferenceScreen
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">
	<CheckBoxPreference 
		android:title="@string/enable_access_control" 
		android:key="enable_access_control" 
		android:defaultValue="false"/>
	<Preference 
		android:title="@string/protect_applicationgs" 
		android:key="protect_applicationgs"/>		
	<CheckBoxPreference 
		android:title="@string/protect_guarded_mode" 
		android:key="protect_guarded_mode" 
		android:defaultValue="false" />
	<CheckBoxPreference 
		android:title="@string/make_pattern_visible" 
		android:key="make_pattern_visible" 
		android:defaultValue="false" />
	<CheckBoxPreference 
		android:title="@string/vibrate_on_touch" 
		android:key="vibrate_on_touch" 
		android:defaultValue="false" />
